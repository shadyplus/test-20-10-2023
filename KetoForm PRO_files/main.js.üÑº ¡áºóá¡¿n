const buttons=document.querySelectorAll(".question__buttons"),allButtons=document.querySelectorAll(".question__buttons-btn");if(window.matchMedia("(max-width: 770px)").matches){function nextQuestion(t){if(t.target.dataset.gender){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.age){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.weight){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.height){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.long){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.keep){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.shape){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.wait){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.deal){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}if(t.target.dataset.figuration){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");setTimeout((()=>{e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}),500)}}buttons.forEach((t=>{t.addEventListener("click",nextQuestion)})),allButtons.forEach((t=>{t.classList.add("bubbly-button")}))}else{function nextQuestion(t){if(t.target.dataset.gender){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}if(t.target.dataset.age){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}if(t.target.dataset.weight){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}if(t.target.dataset.height){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}if(t.target.dataset.long){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");e.classList.remove("question--active"),e.nextElementSibling.classList.add("question--active")}if(t.target.dataset.keep){let e=t.target.closest(".question__buttons").closest(".question__inner").closest(".question");e.classList.remove("question--active"),e.nextElementSib