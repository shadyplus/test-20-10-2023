for(var animateButton=function(t){t.preventDefault,t.target.classList.remove("animate"),t.target.classList.add("animate"),setTimeout((function(){t.target.classList.remove("animate")}),700)},bubblyButtons=document.getElementsByClassName("bubbly-button"),i=0;i<bubblyButtons.length;i++)bubblyButtons[i].addEventListener("click",animateButton,!1);